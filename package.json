{
    "scripts": {
        "clean": "rimraf _site",
        "development": "mix",
        "production": "mix --production",
        "dev": "npm-run-all clean dev:*",
        "dev:webpack": "TAILWIND_MODE=build npm run development",
        "dev:11ty": "ELEVENTY_ENV=development eleventy",
        "watch": "npm-run-all clean --parallel watch:*",
        "watch:webpack": "npm run development watch",
        "watch:11ty": "ELEVENTY_ENV=development eleventy --serve",
        "prod": "npm-run-all clean prod:*",
        "prod:webpack": "npm run production",
        "prod:11ty": "ELEVENTY_ENV=production eleventy"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@vue/compiler-sfc": "^3.0.11",
        "autoprefixer": "^10.2.5",
        "cross-env": "^7.0.3",
        "eslint": "^7.24.0",
        "eslint-plugin-vue": "^7.8.0",
        "eslint-webpack-plugin": "^2.5.3",
        "laravel-mix": "^6.0.16",
        "laravel-mix-eslint": "^0.2.0",
        "npm-run-all": "^4.1.5",
        "postcss-import": "^14.0.1",
        "postcss-nested": "^5.0.5",
        "rimraf": "^2.7.1",
        "tailwindcss": "^2.1.1",
        "vue": "^3.0.11",
        "vue-loader": "^16.2.0"
    }
}
