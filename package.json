{
	"private": true,
	"scripts": {
		"clean": "del-cli _site",
		"local": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=build/webpack.mix",
		"production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=build/webpack.mix",
		"dev": "yarn run clean & yarn run dev:webpack & yarn run dev:11ty",
		"dev:webpack": "yarn run local",
		"dev:11ty": "ELEVENTY_ENV=development eleventy",
		"watch": "yarn run clean & yarn run watch:11ty & yarn run watch:webpack",
		"watch:11ty": "ELEVENTY_ENV=development eleventy --serve",
		"watch:webpack": "yarn run local --watch",
		"prod": "yarn run clean & yarn run prod:webpack & yarn run prod:11ty",
		"prod:webpack": "yarn run production",
		"prod:11ty": "ELEVENTY_ENV=production eleventy"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@braid/vue-formulate": "^2.4.3",
		"@tailwindcss/postcss7-compat": "^2.0.1",
		"browser-sync": "^2.24.6",
		"browser-sync-webpack-plugin": "^2.0.1",
		"cross-env": "^7.0",
		"del-cli": "^3.0.1",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-vue": "^5.2.2",
		"gsap": "file:./resources/assets/js/gsap-bonus.tgz",
		"laravel-mix": "^5.0.1",
		"lodash": "^4.17.13",
		"postcss-units": "^1.2.1",
		"resolve-url-loader": "^3.1.0",
		"sass": "^1.15.2",
		"sass-loader": "^8.0.0",
		"vue": "^2.6.11",
		"vue-template-compiler": "^2.6.11"
	}
}
